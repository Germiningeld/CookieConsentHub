{
    "name": "cookie-consent-hub",
    "version": "1.0.0",
    "description": "A flexible and customizable cookie consent notification system",
    "main": "dist/js/cookie-consent.min.js",
    "scripts": {
      "start": "webpack serve --mode development",
      "build": "webpack --mode production",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:coverage": "jest --coverage",
      "lint": "eslint src/**/*.js",
      "lint:fix": "eslint src/**/*.js --fix",
      "sass": "sass src/sass/main.sass:dist/css/cookie-consent.css --style expanded",
      "sass:watch": "sass src/sass/main.sass:dist/css/cookie-consent.css --style expanded --watch",
      "sass:min": "sass src/sass/main.sass:dist/css/cookie-consent.min.css --style compressed",
      "build:all": "npm run build && npm run sass:min",
      "prepare": "husky install"
    },
    "keywords": [
      "cookie",
      "consent",
      "notification",
      "gdpr",
      "privacy"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
      "@babel/core": "^7.23.0",
      "@babel/preset-env": "^7.22.20",
      "@testing-library/jest-dom": "^6.1.4",
      "@testing-library/user-event": "^14.5.1",
      "babel-jest": "^29.7.0",
      "babel-loader": "^9.1.3",
      "clean-webpack-plugin": "^4.0.0",
      "copy-webpack-plugin": "^11.0.0",
      "css-loader": "^6.8.1",
      "eslint": "^8.51.0",
      "eslint-config-prettier": "^9.0.0",
      "eslint-plugin-jest": "^27.4.2",
      "eslint-plugin-prettier": "^5.0.0",
      "husky": "^8.0.3",
      "jest": "^29.7.0",
      "jest-environment-jsdom": "^29.7.0",
      "lint-staged": "^14.0.1",
      "mini-css-extract-plugin": "^2.7.6",
      "prettier": "^3.0.3",
      "sass": "^1.69.0",
      "sass-loader": "^13.3.2",
      "style-loader": "^3.3.3",
      "terser-webpack-plugin": "^5.3.9",
      "webpack": "^5.88.2",
      "webpack-cli": "^5.1.4",
      "webpack-dev-server": "^4.15.1"
    },
    "lint-staged": {
      "*.js": [
        "eslint --fix",
        "prettier --write"
      ],
      "*.{sass,scss,css}": [
        "prettier --write"
      ]
    },
    "browserslist": [
      "> 0.5%",
      "last 2 versions",
      "not dead"
    ]
  }